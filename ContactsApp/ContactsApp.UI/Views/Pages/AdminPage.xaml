      WindowTitle="Your Contacts"
      Title="Contacts Page"
      Background="Black"
      DataContext="{Binding Path=AdminPageVm, Source={StaticResource Locator}}">
    <Page.Resources>
        
    </Page.Resources>
    <Grid>
        <Label Margin="30"
               Foreground="Firebrick"
               HorizontalContentAlignment="Center"
               Content="Contacts Page"/>
    </Grid>
